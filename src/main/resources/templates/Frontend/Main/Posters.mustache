    <style>

        @font-face {
            font-family: moonget;
            src: url(/font/moon_get-Heavy.ttf);
        }

        .card-body-prod {
            padding: 0;
        }

        .hero--video {
            display: flex;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: flex-end;
            justify-content: space-around;
            flex-wrap: wrap;
            height: 40vh;
        }

        .navbar-fixed-top {
            top: -70px;
            opacity: 0;
        }

        .hero-image {
            position: absolute;
            z-index: 1;
            width: 100%;
            height: 600px;
            margin: 0 auto;
            right: 0;
            left: 0;
            -o-object-fit: cover;
            object-fit: cover;
        }

        .hero .search {
            position: relative;
            vertical-align: middle;
            z-index: 2;
        }

        .hero .search input {
            width: 100%;
            height: 76px;
            line-height: 76px;
            border: 1px solid #111;
            background: rgba(0, 0, 0, 0.8);
            font-size: 27px;
            padding: 5px 5px 5px 47px;
            color: #fff;
            position: relative;
            border-radius: 0;
        }

        .hero .search button {
            position: absolute;
            top: 6px;
            right: 6px;
            color: #fff;
            background: #88687b;
            font-size: 27px;
            height: 65px;
            line-height: 65px;
            width: 68px;
            padding: 0;
            border: 0;
        }

        header {
            width: 100%;
            height: 70vh;
            background: url(/img/hero4.png) no-repeat 50% 50%;
            background-size: cover;
        }

        .content {
            width: 94%;
            margin: 4em auto;
            font-size: 20px;
            line-height: 30px;
            text-align: justify;
        }

        .logo {
            line-height: 60px;
            position: fixed;
            float: left;
            margin: 16px 46px;
            color: #fff;
            font-weight: bold;
            font-size: 20px;
            letter-spacing: 2px;
        }

        .userMenu {
            line-height: 60px;
            position: fixed;
            float: right;
            margin: 16px 46px;
            color: #fff;
            font-weight: bold;
            font-size: 20px;
            letter-spacing: 2px;
        }

        nav {
            position: fixed;
            width: 100%;
            line-height: 60px;
            z-index: 255;
        }

        nav ul {
            line-height: 60px;
            list-style: none;
            background: rgba(0, 0, 0, 0);
            color: #fff;
            padding: 0;
            text-align: right;
            margin: 0;
            padding-right: 40px;
            transition: 1s;
        }

        nav.black ul {
            background: #000;
        }

        nav ul li {
            display: inline-block;
            padding: 16px 40px;;
        }

        nav ul li a {
            text-decoration: none;
            color: #fff;
            font-size: 16px;
        }

        .menu-icon {
            line-height: 60px;
            width: 100%;
            background: #000;
            text-align: right;
            box-sizing: border-box;
            padding: 15px 24px;
            cursor: pointer;
            color: #fff;
            display: none;
        }

        .user2 {
            display: none;
        }

        .card-columns {
            column-count: 4;
        }

        .profile-circel-image-50 img {
            width: 70px;
            height: 70px;
            border-radius: 70px;
            float: left;
            object-fit: cover;

        }

        .profile-circel-image-50 {
            align-items: center;
            justify-content: center;
            display: flex;
        }

        .vv img {
            transition: all 0.3s ease;
            cursor: pointer;
        }

        .vv img:hover {
            opacity: 0.8;
            box-shadow: 0px 0px 5px 8px rgba(0, 0, 0, 0.08);
        }

        .center {
            padding-top: 37vh;
            width: 20%;
            display: inline-block;
            flex-grow: 1;
            align-items: center;

        }

        .center1 {
            padding-top: 22vh;
            width: 20%;
            display: inline-block;
            flex-grow: 1;
            align-items: center;

        }

        .flex-container {

            display: flex;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: flex-end;
            justify-content: space-around;
            flex-wrap: wrap;
            height: 20vh;

        }

      

        .categoryText {
            float: right;
            color: white;
            margin-left: 5%;
            margin-top: 0;
            margin-bottom: 0;
        }

        @media (max-width: 786px) {
            .categoryText {
            }

            .center {
                padding-top: 25vh;

            }

            header {
                width: 100%;
                height: 60vh;
                background: url(/img/hero4.png) no-repeat 50% 50%;
                background-size: cover;
            }

        }

        .categoryText {
            float: right;
            color: white;
            margin-left: 5%;
            margin-top: 0;
            margin-bottom: 0;
        }

        @media (max-width: 786px) {
            .categoryText {
                display: none;
            }

            .center {
                padding-top: 25vh;

            }

            header {
                width: 100%;
                height: 60vh;
                background: url(/img/hero4.png) no-repeat 50% 50%;
                background-size: cover;
            }

            #user1 {
                display: none;
            }

            .user2 {
                display: inline-block;
            }

            .logo {
                position: fixed;
                top: 0;
                margin-top: 16px;
            }

            nav ul {
                max-height: 0px;
                background: #000;
            }

            nav {
                overflow: hidden;
            }

            nav.black ul {
                background: #000;
            }

            .showing {
                max-height: 34em;
                overflow: visible;
            }

            nav ul li {
                box-sizing: border-box;
                width: 100%;
                padding: 5px;
                text-align: center;
            }

            .menu-icon {
                display: block;
            }

            .card-columns {
                column-count: 1;
            }

        }


    </style>


</head>
